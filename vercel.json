{
   "version":2,
   "builds":[
      {
         "src":"api/scrape.js",
         "use":"@vercel/node"
      },
      {
         "src":"api/news.js",
         "use":"@vercel/node"
      },
      {
         "src":"api/channel.js",
         "use":"@vercel/node"
      },
      {
      "src":"api/details.js",
      "use":"@vercel/node"
      },
      {
      "src":"api/targeturl.js",
      "use":"@vercel/node"
      },
      {
         "src":"public/**",
         "use":"@vercel/static"
      }
   ],
   "routes":[
      {
         "src":"/api/scrape",
         "dest":"/api/scrape.js"
      },
      {
         "src":"/api/news",
         "dest":"/api/news.js"
      },
      {
         "src":"/api/channel",
         "dest":"/api/channel.js"
      },
      {
         "src":"/api/details",
         "dest":"/api/details.js"
      },
      {
         "src":"/api/targeturl",
         "dest":"/api/targeturl.js"
      },
      {
         "src":"/(.*)",
         "dest":"/public/$1"
      }
   ]
}
